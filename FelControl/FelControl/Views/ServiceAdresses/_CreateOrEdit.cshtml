@model FelControl.ServiceAdress

@* This partial view defines form fields that will appear when creating and editing entities *@

<div class="control-group">
	@Html.LabelFor(model => model.AdressService, new { @class = "control-label" })
	<div class="controls">
		@Html.EditorFor(model => model.AdressService)
		@Html.ValidationMessageFor(model => model.AdressService)
	</div>
</div>
<div class="control-group">
	@Html.LabelFor(model => model.Service, new { @class = "control-label" })
	<div class="controls">
		@Html.DropDownListFor(model => model.ServiceId, (IEnumerable<SelectListItem>)ViewBag.Service)
		@Html.ValidationMessageFor(model => model.Service)
	</div>
</div>
